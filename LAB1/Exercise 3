<!DOCTYPE html> 
<html lang="en"> 
<head> 
    <script src='https://d3js.org/d3.v7.min.js'></script> 

</head> 

<body> 
    
    <style> 
      .CSSClass { 
        color:'green' 
        
      } 
    </style> 
   
 
<script> 


     
     // EX 3
     for(let i=1;i<=10;i++){
     		if(i<6){
					d3.select("body").append("div").text(i).attr("class","CSSClass")
        }else{
        	d3.select("body").append("div").text(i).style("color", "green")
        }
    }
    
    
</script> 
</body> 
</html> 
